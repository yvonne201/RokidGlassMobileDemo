<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SimpleDemoActivity">

    <Button
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/bt_glass"
        android:text="眼镜硬件信息获取"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <Button
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/bt_glass"
        android:id="@+id/bt_presentation"
        android:text="双屏异显Demo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/bt_presentation"
        android:id="@+id/bt_face_sdk"
        android:text="人脸车牌sdkDemo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/bt_face_sdk"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/bt_online_recg"
        android:text="在线人脸识别Demo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


